<%- include("../includes/header.ejs", {title:"Add New Resume"}); %>
<h1>Add New players</h1>
    <form action="/players/insert" method="get">
        First Name: <input id="first_name" name="first_name" type="text" />
        <br />
        Last Name: <input id="last_name" name="last_name" type="text" />
        <br />
        Year:
        <select id="year" name="year">
            <option value="Freshman">Freshman</option>
            <option value="Sophomore">Sophomore</option>
            <option value="Junior">Junior</option>
            <option value="Senior">Senior</option>
        </select>
        <br />
        Team:
        <select id="team_id" name="team_id">
            <option value="1">Mens Baseball</option>
            <option value="2">Mens Basketball</option>
            <option value="3">Womens Basketball</option>
            <option value="4">Womens Volleyball</option>
        </select>
        Position: <input id="position" name="position" type="text" />
        <br />
        <br />
        <br />

        <input type="submit" />

    </form>
</body>

<%- include("../includes/footer.ejs"); %>