<html>
<head>
    <title>Address</title>
</head>
<body>
<h1>Address</h1>
<table border="1">
    <a href="/address/add">Add New Address</a> <br />
    <br />

    <tr><th>Street</th><th>Zip Code</th></tr>
    <% for(var i=0; result.length > i; i++ ) { %>
    <tr>
        <td><a href="/address/?address_id=<%= result[i].address_id %>"><%= result[i].street %></a></td>
        <td><%= result[i].zip_code %></td>
        <td>
            <form action="/address/delete/">
                <input type="hidden" name="address_id" id="address_id" value="<%= result[i].address_id %>" />
                <input type="submit" value="Delete" />
            </form>
        </td>
    </tr>
    <% } %>
</table>
</body>

</html>